{
  "name": "gemi",
  "version": "0.9.2",
  "devDependencies": {
    "@babel/parser": "^7.25.4",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/bun": "^1.1.10",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "eslint": "^8.57.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "bin": {
    "gemi": "./dist/bin/index.js"
  },
  "exports": {
    "./http": "./http/index.ts",
    "./client": "./client/index.ts",
    "./app": "./app/index.ts",
    "./facades": "./facades/index.ts",
    "./email": "./email/index.ts",
    "./vite": "./dist/vite/index.mjs",
    "./runtime": "./client/runtime.ts",
    "./kernel": "./kernel/index.ts",
    "./services": "./services/index.ts",
    "./broadcasting": "./broadcasting/index.ts"
  },
  "files": [
    "dist/**/*"
  ],
  "private": false,
  "module": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "NODE_ENV=production bun run build:core && bun run build:bin && bun run build:types && bun run build:client && bun run build:plugin && bun run build:client-types",
    "build:core": "bun ./scripts/build.ts",
    "build:bin": "bun build ./bin/gemi.ts --target bun --external vite --external react-dom --external react/jsx-runtime --external bun --external sharp --sourcemap --outfile ./dist/bin/index.js && bun ./scripts/prepare-bin.ts",
    "build:client": "vite build",
    "build:plugin": "vite build -c vite.plugin.config.mts",
    "build:types": "tsc",
    "build:client-types": "tsc -p tsconfig.browser.json",
    "test": "vitest",
    "prepublishOnly": "bun run build && bun scripts/setupExports.ts prod",
    "postpublish": "bun scripts/setupExports.ts dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.629.0",
    "@aws-sdk/s3-request-presigner": "^3.629.0",
    "@prisma/client": "5.17.0",
    "ast-types": "^0.14.2",
    "commander": "^12.1.0",
    "history": "^5.3.0",
    "jsx-email": "2.0.0-rc2.1",
    "open": "^10.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recast": "^0.23.9",
    "resend": "^3.4.0",
    "temporal-polyfill": "^0.2.5",
    "urlpattern-polyfill": "^10.0.0",
    "uuid": "^9.0.1"
  }
}
